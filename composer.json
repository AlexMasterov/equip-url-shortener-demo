{
  "name": "alexmasterov/url-shortener-demo",
  "description": "Demo of URL Shortener Service",
  "require": {
    "php": "^5.5.9 || ^7.0",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "equip/framework": "dev-master as 2.0",
    "zendframework/zend-diactoros": "^1.3",
    "josegonzalez/dotenv": "^2.0",
    "paragonie/random_compat": "^2.0",
    "alexmasterov/equip-twig": "^2.0",
    "alexmasterov/twig-extension": "^0"
  },
  "autoload": {
    "psr-4": {
      "UrlShortener\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UrlShortener\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "sqlite": "php bin/sqlite",
    "ui": "cd ui && npm i && npm run build"
  }
}
